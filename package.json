{
  "name": "workisy",
  "version": "1.0.0",
  "description": "APIS workisy",
  "main": "index.js",
  "scripts": {
    "start-prod": "npm run build && cross-env NODE_ENV=production nodemon dist/server.ts",
    "start:dev": "npm run build && cross-env NODE_ENV=development nodemon dist/server.js",
    "start:local": "npm run build && cross-env NODE_ENV=local nodemon dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "local": "cross-env NODE_ENV=local nodemon",
    "build": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "test:dev": "cross-env NODE_ENV=development jest  --detectOpenHandles --forceExit",
    "test:local": "cross-env NODE_ENV=local jest  --detectOpenHandles --forceExit",
    "test:prod": "cross-env NODE_ENV=production jest --forceExit --detectOpenHandles ",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rm -rf dist && rm -rf src/logs",
    "postinstall": "npm run build",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only workisy-api",
    "deploy:dev": "npm run build && pm2 start ecosystem.config.js --only workisy-api-dev"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.23",
    "@swc/jest": "^0.2.36",
    "@types/async": "^3.2.24",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/connect-timeout": "^0.0.39",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/cron": "^2.4.0",
    "@types/dotenv-safe": "^8.1.6",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.6",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.5.3",
    "@types/node-cron": "^3.0.11",
    "@types/nodemon": "^1.19.6",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "cross-env": "^7.0.3",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "node-config": "^0.0.2",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@cloudinary-util/types": "^1.5.6",
